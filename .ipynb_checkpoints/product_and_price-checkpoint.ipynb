    "# Product 記帳程式\n",
    "\n",
    "products =[]\n",
    "\n",
    "while True:\n",
    "    name = input(\"請輸入商品名稱：\")\n",
    "    if name == \"q\":\n",
    "        break\n",
    "    price = input(\"請輸入商品價錢：\")\n",
    "    p = []\n",
    "    p.append(name)\n",
    "    p.append(price)\n",
    "    products.append(p)\n",
    "\n",
    "print(products)"
   ]
  }
